//Array-1 > firstLast6
public boolean firstLast6(int[] nums) {
  int len = nums.length;
    return (len >= 1 && (nums[0] == 6 || nums[len-1] == 6));
}

//Array-1 > sameFirstLast
public boolean sameFirstLast(int[] nums) {
  int len = nums.length;
  return (len >= 1 && nums[0] == nums[len-1]);
}

//Array-1 > makePi 
public int[] makePi() {
  int [] pie = {3, 1, 4};
  return pie;
} 
